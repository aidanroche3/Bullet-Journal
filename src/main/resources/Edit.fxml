<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane styleClass="page" prefHeight="415.0" prefWidth="454.0" stylesheets="@style.css">
      <children>
         <Label layoutX="147.0" layoutY="27.0" text="Preferences">
            <font>
               <Font size="30.0" />
            </font>
         </Label>
         <Button fx:id="cancel" styleClass="btn" layoutX="74.0" layoutY="335.0" mnemonicParsing="false" text="Cancel">
            <font>
               <Font size="25.0" />
            </font>
         </Button>
         <Button fx:id="confirm" styleClass="btn" layoutX="269.0" layoutY="335.0" mnemonicParsing="false" text="Confirm">
            <font>
               <Font size="25.0" />
            </font>
         </Button>
         <Slider fx:id="taskSlider" blockIncrement="1.0" layoutX="30.0" layoutY="189.0" majorTickUnit="1.0" max="20.0" min="1.0" minorTickCount="2" prefHeight="38.0" prefWidth="394.0" showTickLabels="true" showTickMarks="true" value="1.0" />
         <Label fx:id="taskLimit" layoutX="147.0" layoutY="150.0" text="Task Limit: 0">
            <font>
               <Font size="20.0" />
            </font>
         </Label>
         <Slider fx:id="eventSlider" blockIncrement="1.0" layoutX="29.0" layoutY="278.0" majorTickUnit="1.0" max="20.0" min="1.0" minorTickCount="2" prefHeight="38.0" prefWidth="394.0" showTickLabels="true" showTickMarks="true" value="1.0" />
         <Label fx:id="eventLimit" layoutX="144.0" layoutY="240.0" text="Event Limit: 0">
            <font>
               <Font size="20.0" />
            </font>
         </Label>
         <Label layoutX="196.0" layoutY="74.0" text="Name">
            <font>
               <Font size="20.0" />
            </font>
         </Label>
         <TextField fx:id="name" layoutX="142.0" layoutY="112.0" />
      </children></AnchorPane>
</Scene>
